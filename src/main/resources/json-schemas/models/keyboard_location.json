{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Keyboard",
  "type": "object",
  "properties": {
    "host_keyboard": {
      "description": "Keyboard which contains current one",
      "type": "string"
    },
    "link_button": {
      "description": "Button on host keyboard which leads to current one",
      "type": "string"
    }
  },
  "required": [
    "host_keyboard",
    "link_button"
  ],
  "additionalProperties": false
}