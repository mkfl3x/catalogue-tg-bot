{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Delete keyboard request",
  "type": "object",
  "properties": {
    "keyboard_name": {
      "description": "Name of keyboard to delete",
      "type": "string",
      "minLength": 1
    },
    "recursively": {
      "description": "Delete all nested keyboards and buttons",
      "type": "boolean"
    }
  },
  "required": [
    "keyboard_name",
    "recursively"
  ],
  "additionalProperties": false
}